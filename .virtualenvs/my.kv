<ScanningWindow>:
    name: "scanning"
    display: entry
    FloatLayout:
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.10, 'top': 1}
            background_color: (0, 204/255.0, 255/255.0 ,1.0)
			text: "Home"
			on_release:
            	app.root.current = "main"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.30, 'top': 1}
            background_color: (0, 165/255.0, 207/255.0, 1.0)
			text: "Patching"
			on_release:
            	app.root.current = "patching"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.50, 'top': 1}
            background_color: (0, 120/255.0, 151/255.0, 1.0)
			text: "Network"
			on_release:
