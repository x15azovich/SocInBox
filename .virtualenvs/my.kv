WindowManager:
	MainWindow:
	PatchingWindow:
    NetworkWindow:
    ScanningWindow:
    HostbaseWindow:

<MainWindow>:
    name: "main"
    FloatLayout:
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.10, 'top': 1}
            background_color: (0, 8/255.0, 42.5/255.0, .550 )
			text: "Home"
			on_release:
            	app.root.current = "main"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.30, 'top': 1}
            background_color: (0/255.0, 200/255.0, 255/255.0, 1.0)
			text: "Patching"
			on_release:
            	app.root.current = "patching"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.50, 'top': 1}
            background_color: (0, 90/255.0, 151/255.0, 1.0)
			text: "Network"
			on_release:
            	app.root.current = "network"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.70, 'top': 1}
            background_color: (0, 60/255.0, 110.5/255.0, .550 )
			text: "Scanning"
			on_release:
            	app.root.current = "scanning"
		Button:
            size_hint: 0.2,0.10
            pos_hint : {'center_x' : 0.90, 'top': 1}
            background_color: (0, 30/255.0, 75/255.0, 1.0)
			text: "Hostbase"
			on_release:
            	app.root.current = "hostbase"


