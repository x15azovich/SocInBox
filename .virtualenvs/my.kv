            background_color: (0, 80/255.0, 110/255.0, 1.0)
			text: "Hostbase"
			on_release:
            	app.root.current = "hostbase"

        TextInput:
            size_hint: 0.70,0.075
            pos_hint : {'center_x' : 0.40, 'top': .80}
            hint_text : "Enter IP address here:"
            id: entry
            font_size: 32
            multiline: False
        Button:
            size_hint: 0.1, 0.075
            pos_hint : {'center_x' : 0.85, 'top': .80}
            background_color: (0, 204/255.0, 255/255.0 ,1.0)
            text: "Scan"
            on_press: root.press_scan(entry.text)

        #Label:
        #    text: "Vulnerabilities:"
            


